<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="tab_button">
        <attr name="tabtext" format="reference|string"/>
        <attr name="txtSize" format="reference|dimension" />
        <attr name="drawableTop" format="reference"/>
        <attr name="drawableTopAttr" format="string"/>
        <attr name="drawableBottom" format="reference"/>
    </declare-styleable>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="tabPaddingLeftRight" format="dimension"/>
        <attr name="backgroundTab" format="reference"/>
        <attr name="textIndicatorColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="SlideBar">
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
        <attr name="slideBar_chooseTextColor" format="color"/>
        <attr name="slideBar_backgroundColor" format="color"/>
        <attr name="slideBar_chooseBackgroundColor" format="color"/>
        <attr name="slideBar_style">
            <enum name="default_" value="0"/>
            <enum name="none" value="1"/>
            <enum name="circle" value="2"/>
            <enum name="stretch" value="3"/>
        </attr>
    </declare-styleable>
</resources>